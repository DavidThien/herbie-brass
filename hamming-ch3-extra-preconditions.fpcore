;; -*- mode: scheme -*-

(FPCore (x)
 :name "NMSE example 3.1"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 x 1e20)
 (- (sqrt (+ x 1)) (sqrt x)))

(FPCore (x eps)
 :name "NMSE example 3.3"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< -1e-10 eps 1e-10) (< (- PI) x PI))
 (- (sin (+ x eps)) (sin x)))
 
(FPCore (x)
 :name "NMSE example 3.4"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e-20 x 1e-10)
 (/ (- 1 (cos x)) (sin x)))

(FPCore (N)
 :name "NMSE example 3.5"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 N 1e20)
 (- (atan (+ N 1)) (atan N)))
 
(FPCore (x)
 :name "NMSE example 3.6"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 x 1e20)
 (- (/ 1 (sqrt x)) (/ 1 (sqrt (+ x 1)))))

(FPCore (x)
 :name "NMSE problem 3.3.1"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 x 1e20)
 (- (/ 1 (+ x 1)) (/ 1 x)))

(FPCore (x eps)
 :name "NMSE problem 3.3.2"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< (- PI) x PI) (< -1e-10 eps 1e-10))
 (- (tan (+ x eps)) (tan x)))
 
(FPCore (x)
 :name "NMSE problem 3.3.3"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 x 1e20)
 (+ (- (/ 1 (+ x 1)) (/ 2 x)) (/ 1 (- x 1))))

(FPCore (x)
 :name "NMSE problem 3.3.4"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 x 1e20)
 (- (pow (+ x 1) (/ 1 3)) (pow x (/ 1 3))))

(FPCore (x eps)
 :name "NMSE problem 3.3.5"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< (- PI) x PI) (< -1e-10 eps 1e-10))
 (- (cos (+ x eps)) (cos x)))
 
(FPCore (N)
 :name "NMSE problem 3.3.6"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e10 N 1e20)
 (- (log (+ N 1)) (log N)))

(FPCore (x)
 :name "NMSE problem 3.3.7"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< 1e-10 x 1)
