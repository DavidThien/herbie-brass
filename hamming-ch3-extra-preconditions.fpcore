(FPCore (a b c)
 :name "NMSE p42, positive"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< 1e10 b 1e20) (< -1 a 1) (< -1 c 1))
 (/ (+ (- b) (sqrt (- (* b b) (* 4 (* a c))))) (* 2 a)))

(FPCore (a b c)
 :name "NMSE p42, negative"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< -1e20 b -1e10) (< -1 a 1) (< -1 c 1))
 (/ (- (- b) (sqrt (- (* b b) (* 4 (* a c))))) (* 2 a)))

(FPCore (a b2 c)
 :name "NMSE problem 3.2.1, positive"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< 1e10 b2 1e20) (< -1 a 1) (< -1 c 1))
 (/ (+ (- b2) (sqrt (- (* b2 b2) (* a c)))) a))

(FPCore (a b2 c)
 :name "NMSE problem 3.2.1, negative"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (and (< -1e20 b2 -1e10) (< -1 a 1) (< -1 c 1))
 (/ (- (- b2) (sqrt (- (* b2 b2) (* a c)))) a))

(FPCore (x)
 :name "NMSE example 3.7"
 :cite (hamming-1987 herbie-2015)
 :fpbench-domain textbook
 :pre (< -1e-10 x 1e-10)
